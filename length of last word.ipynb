   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Length of last word: 3\n"
     ]
    }
   ],
   "source": [
    "def length_of_last(A):\n",
    "    arr = A.strip().split()\n",
    "\n",
    "    if not arr:\n",
    "        return 0\n",
    "\n",
    "    return len(arr[-1])\n",
    "\n",
    "A = input(\"Enter the string you want: \")\n",
    "print(\"Length of last word:\", length_of_last(A))"
   ]
